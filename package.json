{
  "name": "@polymer-labs/delta",
  "version": "0.0.1",
  "description": "Stencil Component Starter",
  "homepage": "https://github.com/PolymerLabsTeam/delta-core#readme",
  "bugs": {
    "url": "https://github.com/PolymerLabsTeam/delta-core/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/PolymerLabsTeam/delta-core.git"
  },
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/delta-lib/delta-lib.esm.js",
  "exports": {
    ".": {
      "import": "./dist/components/index.js",
      "types": "./dist/components/index.d.ts"
    },
    "./d-button": {
      "import": "./dist/components/d-button.js",
      "types": "./dist/components/d-button.d.ts"
    }
  },
  "types": "./dist/components/index.d.ts",
  "files": [
    "dist/",
    "loader/"
  ],
  "scripts": {
    "clean": "rm -rf dist/",
    "build": "stencil build",
    "start": "stencil build --dev --watch",
    "test": "stencil test --spec --e2e",
    "test.watch": "stencil test --spec --e2e --watchAll",
    "generate": "stencil generate",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "dependencies": {
    "@phosphor-icons/core": "^2.1.1"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^3.2.5",
    "@stencil/core": "^4.27.1",
    "@stencil/sass": "^3.1.9",
    "@storybook/addon-essentials": "^8.6.4",
    "@storybook/blocks": "^8.6.4",
    "@storybook/experimental-addon-test": "^8.6.4",
    "@storybook/test": "^8.6.4",
    "@storybook/web-components": "^8.6.4",
    "@storybook/web-components-vite": "^8.6.4",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.13.5",
    "jest": "^29.7.0",
    "jest-cli": "^29.7.0",
    "lit": "^3.2.1",
    "puppeteer": "^24.3.0",
    "storybook": "^8.6.4"
  }
}
